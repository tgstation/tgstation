/mob/living/silicon/emp_act(severity)
	switch(severity)
		if(1)
			src.take_organ_damage(40)
			Stun(rand(5,10))
		if(2)
			src.take_organ_damage(20)
			Stun(rand(1,5))
	flick("noise", src:flash)
	src << "\red <B>*BZZZT*</B>"
	src << "\red Warning: Electromagnetic pulse detected."
	..()

