{
  "private": true,
  "name": "tgui-workspace",
  "version": "4.2.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "eslint packages && webpack-cli --mode=production",
    "webpack": "webpack-cli",
    "watch": "node packages/tgui-dev-server/index.esm.js",
