{
	"private": true,
	"name": "tgui-workspace",
	"version": "6.0.0",
	"packageManager": "bun@1.2.16",
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"tgui:analyze": "webpack --analyze",
		"tgui:build": "webpack",
		"tgui:dev": "bun packages/tgui-dev-server/index.ts",
		"tgui:lint": "eslint packages --ext .js,.cjs,.ts,.tsx",
		"tgui:prettier": "prettier --check .",
		"tgui:test": "bun test",
		"tgui:tsc": "tsc",
		"tgui:prettier-fix": "prettier --write .",
		"tgui:eslint-fix": "eslint --fix packages --ext .js,.cjs,.ts,.jsx,.tsx"
	},
	"dependencies": {
		"@swc/core": "^1.12.1",
		"@typescript-eslint/parser": "^8.13.0",
		"@typescript-eslint/utils": "^8.13.0",
		"css-loader": "^7.1.2",
		"esbuild-loader": "^4.3.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-react": "^7.34.1",
		"eslint-plugin-simple-import-sort": "^12.0.0",
		"eslint-plugin-unused-imports": "^3.1.0",
		"mini-css-extract-plugin": "^2.9.2",
		"prettier": "^3.5.3",
		"sass": "^1.80.6",
		"sass-loader": "^16.0.3",
		"swc-loader": "^0.2.6",
		"typescript": "^5.8.3",
		"webpack": "^5.99.9",
		"webpack-bundle-analyzer": "^4.10.2",
		"webpack-cli": "^6.0.1"
	},
	"devDependencies": {
		"@happy-dom/global-registrator": "^17.4.7",
		"@types/bun": "^1.2.14",
		"@types/react": "^19.1.5",
		"@types/react-dom": "^19.1.5",
		"@types/webpack-env": "^1.18.8",
		"@types/wicg-file-system-access": "^2023.10.6"
	}
}
